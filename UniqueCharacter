import java.util.Scanner;
public class UniqueCharacter {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a word or phrase: ");
        String text = sc.nextLine();        
        String processed = text.toLowerCase().replace(" ", "");        
        boolean hasDuplicate = false;
        for (int i = 0; i < processed.length(); i++) {
            for (int j = i + 1; j < processed.length(); j++) {
                if (processed.charAt(i) == processed.charAt(j)) {
                    hasDuplicate = true;
                    break;
                }
            }
            if (hasDuplicate) break;
        }        
        if (!hasDuplicate) {
            System.out.println("PERFECT");
            System.out.println("Reason: \"" + text + "\" does not have any character repeating");
        } else {
            System.out.println("NOT PERFECT");
            System.out.println("Reason: \"" + text + "\" has repeating character(s)");
        }

        
    }
}
